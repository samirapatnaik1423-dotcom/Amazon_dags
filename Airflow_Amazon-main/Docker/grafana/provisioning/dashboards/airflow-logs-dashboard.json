{
  "dashboard": {
    "title": "Airflow ETL Pipeline Logs",
    "uid": "airflow-logs-dashboard",
    "tags": ["airflow", "logs", "etl"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "30s",
    "panels": [
      {
        "id": 1,
        "title": "Master Orchestrator Logs",
        "type": "logs",
        "datasource": {
          "type": "loki",
          "uid": "loki"
        },
        "targets": [
          {
            "expr": "{dag_id=\"etl_master_orchestrator\"} |= ``",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      },
      {
        "id": 2,
        "title": "Stage Start/Complete Logs",
        "type": "logs",
        "datasource": {
          "type": "loki",
          "uid": "loki"
        },
        "targets": [
          {
            "expr": "{dag_id=\"etl_master_orchestrator\"} |~ \"STAGE (START|COMPLETE)\"",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 12
        },
        "options": {
          "showTime": true,
          "showLabels": false,
          "wrapLogMessage": true,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        }
      },
      {
        "id": 3,
        "title": "All DAG Logs (Filterable)",
        "type": "logs",
        "datasource": {
          "type": "loki",
          "uid": "loki"
        },
        "targets": [
          {
            "expr": "{job=\"airflow-dags\"} |= ``",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 12,
          "w": 24,
          "x": 0,
          "y": 22
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "wrapLogMessage": true,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        }
      },
      {
        "id": 4,
        "title": "Error & Warning Logs",
        "type": "logs",
        "datasource": {
          "type": "loki",
          "uid": "loki"
        },
        "targets": [
          {
            "expr": "{job=\"airflow-dags\"} |~ \"(ERROR|WARNING|FAILED|Exception)\"",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 34
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "wrapLogMessage": true,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        }
      },
      {
        "id": 5,
        "title": "Data Processing Metrics (from logs)",
        "type": "logs",
        "datasource": {
          "type": "loki",
          "uid": "loki"
        },
        "targets": [
          {
            "expr": "{dag_id=\"etl_master_orchestrator\"} |~ \"Data: .* extracted\"",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 44
        },
        "options": {
          "showTime": true,
          "showLabels": false,
          "wrapLogMessage": false,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        }
      },
      {
        "id": 6,
        "title": "Scheduler Logs",
        "type": "logs",
        "datasource": {
          "type": "loki",
          "uid": "loki"
        },
        "targets": [
          {
            "expr": "{job=\"airflow-scheduler\"} |= ``",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 0,
          "y": 52
        },
        "options": {
          "showTime": true,
          "showLabels": false,
          "wrapLogMessage": true,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        }
      },
      {
        "id": 7,
        "title": "DAG Processor Logs",
        "type": "logs",
        "datasource": {
          "type": "loki",
          "uid": "loki"
        },
        "targets": [
          {
            "expr": "{job=\"airflow-dag-processor\"} |= ``",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 10,
          "w": 12,
          "x": 12,
          "y": 52
        },
        "options": {
          "showTime": true,
          "showLabels": false,
          "wrapLogMessage": true,
          "enableLogDetails": true,
          "sortOrder": "Descending"
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "dag_id",
          "type": "query",
          "label": "DAG ID",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "query": "label_values(dag_id)",
          "multi": true,
          "includeAll": true,
          "refresh": 1,
          "allValue": ".*"
        },
        {
          "name": "task_id",
          "type": "query",
          "label": "Task ID",
          "datasource": {
            "type": "loki",
            "uid": "loki"
          },
          "query": "label_values({dag_id=~\"$dag_id\"}, task_id)",
          "multi": true,
          "includeAll": true,
          "refresh": 1,
          "allValue": ".*"
        }
      ]
    },
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    }
  },
  "overwrite": true
}
